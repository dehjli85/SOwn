  <td class="score_cell"><%= display_name %></td>
  <% for( var i = 0; i < parentActivities.length; i++){ %>


    <td class="score_cell">
      <% if (parentActivities[i].activity_type == 'scored' ){ %>
        <input class="score_input" id="studentPerformance_<%= parentActivities[i].classroom_activity_pairing_id %>_<%= id %>" name="studentPerformance[<%= parentActivities[i].classroom_activity_pairing_id %>][<%= id %>]" type="text" value="<% if(student_performance[i]){ %> <%= student_performance[i].performance_pretty %> <% }else{''} %>">
        
        
      <% }else if(parentActivities[i].activity_type === 'completion' ){ %>
        <select id="studentPerformance_<%= parentActivities[i].classroom_activity_pairing_id %>_<%= id %>" name="studentPerformance[<%= parentActivities[i].classroom_activity_pairing_id %>][<%= id %>]">
          <option></option>
          <option value="true" <% if(student_performance[i] && student_performance[i].completed_performance=='t'){ %> selected="selected" <%}%> >Completed</option>
          <option value="false" <% if(student_performance[i] && student_performance[i].completed_performance =='f'){ %> selected="selected" <%}%> >Not Completed</option>
        </select>
        
      <% } %>
    </td>



<% } %>


