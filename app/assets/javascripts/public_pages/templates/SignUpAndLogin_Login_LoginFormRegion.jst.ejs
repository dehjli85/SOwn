<form action="/login_post" method="post" class="form-signin" ui-credentials-form>
  <div style="display:none">
    <input name="utf8" type="hidden" value="âœ“">
    <input name="authenticity_token" type="hidden" value="<%= $('meta[name="csrf-token"]').attr('content') %>">
  </div>
  <h4 class="heading-mini">Log In</h4><hr>
  <div class="row">
    <div>
      <div class="form-group form-group_sign-up" ui-google-sign-up-div>         
        <div class="col-md-8 col-md-offset-2">
          <a id="sign_in" class="btn btn-lg btn-block theme-btn-color btn-google-plus btn_sign-up" data-toggle="tooltip" data-placement="right" title="Waiting for Google..." disabled ui-google-log-in-button><i class="fa fa-google-plus"></i>  Log In with Google</a>

        </div>
      </div>

      <div class="form-group" ui-or-div>
        <div class="col-md-8 col-md-offset-2">
          <div class="center">OR</div>
        </div>
      </div>

      <div class="form-group form-group_sign-up" ui-create-account-div>         
        <div class="col-md-8 col-md-offset-2">
          <a class="btn btn-lg theme-btn-color btn-block js-sign-up btn_sign-up" ui-sign-in-account-button>Log in with Sown To Grow Account</a>
        </div>
      </div>  
    </div>
      <div ui-standard-form-div style="display:none">
        <div class="form-group" ui-email-form-group-div>
          <div class="col-md-6 margin-btm-20" ui-email-input-div>
            <input type="email" class="form-control" placeholder="Email.." name="user[email]" ui-email-input autofocus>
          </div>
        </div>
        <div class="form-group" ui-password-form-group-div>
          <div class="col-md-6 margin-btm-20" ui-password-input-div>
            <input type="password" class="form-control" placeholder="Password..." name="user[password]">
          </div>
        </div>
        <div class="form-group">
          <div class="col-md-8 col-md-offset-2">
            <button class="btn btn-lg theme-btn-color btn-block js-sign-in" type="submit">Log in</button>
          </div>
        </div>
      </div>
        
        
  </div>
  <hr>
    <div class="form-btm-link text-center">
     Need to sign up?  Are you a <a href="#" ui-teacher-sign-up-link>teacher</a> or <a href="#" ui-student-sign-up-link>student</a>?
    </div>
</form>